<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  </head>
  <body>
    <h1><%= title %></h1>
    <form>
      <a href="/add/" class="btn btn-default">Add</a> </button>
    </form>

    <table class="table">
      <thead>
        <tr>
          <th>No</th>
          <th>NAME</th>
          <th>MEMBER ID</th>
          <th>ADDRESS</th>
          <th>ZIPCODE</th>
          <th>PHONE</th>
        </tr>
      </thead>
      <tbody>
        <% for (var i=0;i < custData.length;i++) { %>
        <tr>
          <th scope="row"> <%= i + 1 %> </th>
          <td><%= custData[i].name %></td>
          <td><%= custData[i].memberid %></td>
          <td><%= custData[i].address %></td>
          <td><%= custData[i].zipcode %></td>
          <td><%= custData[i].phone %></td>
          <td><a href="/update/<%= custData[i].id %>" class="btn btn-info">update</a>
             <a href="/delete/<%= custData[i].id %>" onclick="return confirm('hapus title: <%= custData[i].title %>')" class="btn btn-danger">delete</a> </td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
